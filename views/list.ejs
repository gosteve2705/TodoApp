
<%- include('header'); -%>
    <div class="box" id="heading">
      <h1> <%= listTitle  %></h1>
    </div>
    <div class="box" >
      <% for(task in newTask){ %>
      <div class="item">
        <input type="checkbox">
          <p> <%= newTask[task] %></p>
      </div>
        <% } %>

        <form  class="item" action="/" method="post">

          <input type="text" name="task"  placeholder="New Task" autocomplete="off" >
          <button type="submit" name="tasksList" value=<%= listTitle %>>+</button>
        </form>
    </div>
<%- include('footer'); -%>
